#Auto-generated config file for voice florent_fr_hmm

name = florent_fr_hmm
fr-voice.version = 4.3.0

voice.version = 4.3.0

# Declare "group names" as component that other components can require.
# These correspond to abstract "groups" of which this component is an instance.
provides = \
         fr-voice \
         hmm-voice

# List the dependencies, as a whitespace-separated list.
# For each required component, an optional minimum version and an optional
# download url can be given.
# We can require a component by name or by an abstract "group name"
# as listed under the "provides" element.
requires = \
   fr \
   marybase 

requires.marybase.version = 4.3.0
requires.fr.version = 4.3.0
requires.fr.download = http://mary.dfki.de/download/mary-install-4.x.x.jar
requires.hmm.version = 4.3.0

####################################################################
####################### Module settings  ###########################
####################################################################
# For keys ending in ".list", values will be appended across config files,
# so that .list keys can occur in several config files.
# For all other keys, values will be copied to the global config, so
# keys should be unique across config files.

hmm.voices.list = \
   florent_fr_hmm

# If this setting is not present, a default value of 0 is assumed.
voice.florent_fr_hmm.wants.to.be.default = 0

# Set your voice specifications
voice.florent_fr_hmm.gender = male
voice.florent_fr_hmm.locale = fr
voice.florent_fr_hmm.domain = general
voice.florent_fr_hmm.samplingRate = 16000

# HMM Voice-specific parameters 
# parameters used during models training 
# MGC: stage=gamma=0 alpha=0.42 linear gain (default) 
# LSP: gamma>0  
#          LSP: gamma=1 alpha=0.0  linear gain/log gain 
#      Mel-LSP: gamma=1 alpha=0.42 log gain 
#      MGC-LSP: gamma=3 alpha=0.42 log gain 
voice.florent_fr_hmm.alpha = 0.42
voice.florent_fr_hmm.gamma = 0
voice.florent_fr_hmm.logGain = false

# Parameter beta for postfiltering 
voice.florent_fr_hmm.beta = 0.0

# HMM Voice-specific files
# Information about trees
voice.florent_fr_hmm.Ftd = MARY_BASE/lib/voices/florent_fr_hmm/tree-dur.inf
voice.florent_fr_hmm.Ftf = MARY_BASE/lib/voices/florent_fr_hmm/tree-lf0.inf
voice.florent_fr_hmm.Ftm = MARY_BASE/lib/voices/florent_fr_hmm/tree-mgc.inf
voice.florent_fr_hmm.Fts = MARY_BASE/lib/voices/florent_fr_hmm/tree-str.inf

# Information about means and variances PDFs 
voice.florent_fr_hmm.Fmd = MARY_BASE/lib/voices/florent_fr_hmm/dur.pdf
voice.florent_fr_hmm.Fmf = MARY_BASE/lib/voices/florent_fr_hmm/lf0.pdf
voice.florent_fr_hmm.Fmm = MARY_BASE/lib/voices/florent_fr_hmm/mgc.pdf
voice.florent_fr_hmm.Fms = MARY_BASE/lib/voices/florent_fr_hmm/str.pdf

# Information about Global Mean and Variance PDFs
voice.florent_fr_hmm.useGV = true
voice.florent_fr_hmm.maxMgcGvIter = 200
voice.florent_fr_hmm.maxLf0GvIter = 200
voice.florent_fr_hmm.Fgvf = MARY_BASE/lib/voices/florent_fr_hmm/gv-lf0-littend.pdf
voice.florent_fr_hmm.Fgvm = MARY_BASE/lib/voices/florent_fr_hmm/gv-mgc-littend.pdf
voice.florent_fr_hmm.Fgvs = MARY_BASE/lib/voices/florent_fr_hmm/gv-str-littend.pdf

# A context features file example for start-up testing.
voice.florent_fr_hmm.FeaFile = MARY_BASE/lib/voices/florent_fr_hmm/utt_2513.pfeats

# Tricky phones file in case there were problematic phones during training, empty otherwise.
voice.florent_fr_hmm.trickyPhonesFile = MARY_BASE/lib/voices/florent_fr_hmm/trickyPhones.txt

# Information about Mixed Excitation
voice.florent_fr_hmm.useMixExc = true

# Filters for mixed excitation 
# File format: one column with all the taps, where the number of taps per filter = numTaps/numFilters 
voice.florent_fr_hmm.Fif = MARY_BASE/lib/voices/florent_fr_hmm/mix_excitation_filters.txt
# Number of filters in bandpass bank
voice.florent_fr_hmm.in = 5

# Information about acousticModels (if true allows prosody modification specified in MARYXML input)
voice.florent_fr_hmm.useAcousticModels = true

# acoustic models to use (HMM models or carts from other voices can be specified)
#(uncoment to allow prosody modification specified in MARYXML input)
voice.florent_fr_hmm.acousticModels = duration F0

voice.florent_fr_hmm.duration.model = hmm
voice.florent_fr_hmm.duration.data = MARY_BASE/conf/fr-florent_fr_hmm.config
voice.florent_fr_hmm.duration.attribute = d

voice.florent_fr_hmm.F0.model = hmm
voice.florent_fr_hmm.F0.data = MARY_BASE/conf/fr-florent_fr_hmm.config
voice.florent_fr_hmm.F0.attribute = f0

